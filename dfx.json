{
  "canisters": {
    "storage": {
      "main":"src/backend/storage/Container.mo",
      "type":"motoko"
    },
    "bucket":{
      "main":"src/backend/storage/Buckets.mo",
      "type":"motoko"
    },
    "graphql": {
      "type": "custom",
      "build": "cargo build --target wasm32-unknown-unknown --package graphql --release",
      "candid": "src/backend/graphql/src/graphql.did",
      "wasm": "target/wasm32-unknown-unknown/release/graphql.wasm"
    },
    "playground": {
      "dependencies": ["graphql"],
      "type": "assets",
      "source": ["src/backend/playground/build"]
    },
    "assets": {
      "dependencies": ["storage"],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "dfx": "0.8.3",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}